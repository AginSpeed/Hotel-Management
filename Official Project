void load()
{
 clrscr();
 gotoxy(30,12);
 cout<<" __________________________ ";
 gotoxy(30,13);
 cout<<"|                          |";
 gotoxy(30,14);
 cout<<" -------------------------- ";
 int k=31;
 for(int i=0;i<101;i++)
 {
  for(int j=0;j<10;j++)
  {
   if(i<100)
   {
		gotoxy(42,15);
		cout<<i<<"."<<j<<" %";
    slow();
	 }
	 else
	 {
		gotoxy(42,15);
		cout<<i<<".0 %";
	 }
	}
  if(i%4==0)
  {
   gotoxy(k,13);
	 cout<<"";
	 k++;
  }
 }
 clrscr();
 int b=1;
 char a;
 gotoxy(1,23);
 for(i=0;i<80;i++)
 cout<<"_";
 gotoxy(1,24);
 cout<<"***************************|PRESS ENTER TO CONTINUE|***************************";
 while(b)
 {
  if(kbhit())
	{
   a=getch();
   if(a==13)
	 break;
	}
	if(b<5)
  {
   gotoxy(33,12);
	 cout<<"Loading...";
   PrintCircle(38,12);
	}
	else
	{
	 gotoxy(33,12);
   cout<<"System Loaded!";
   PrintCircle(38,12);
	}
	b++;
 }
}
